{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST"
      class="form-signin">
    <h2 class="form-signin-heading">{{ 'New password'|trans() }}</h2>

    {{ form_widget(form) }}
    <div>
        <input type="submit" class="btn btn-lg btn-primary btn-block" value="{{ 'resetting.reset.submit'|trans }}"/>
    </div>
</form>
